{
    "name": "laravel/forge-cli",
    "description": "The Laravel Forge CLI.",
    "keywords": ["laravel", "forge", "cli"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/forge-cli/issues",
        "source": "https://github.com/laravel/forge-cli"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "phpseclib/phpseclib": "^3.0.19"
    },
    "require-dev": {
        "laravel-zero/framework": "^10.0.2",
        "laravel/forge-sdk": "^3.13.4",
        "mockery/mockery": "^1.5.1",
        "larastan/larastan": "^2.6.0",
        "pestphp/pest": "^2.6.1",
        "spatie/once": "^3.1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1.0"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/forge"
    ]
}
